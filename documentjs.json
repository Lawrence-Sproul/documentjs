{
	"sites": {
		"docs": {
			"parent": "DocumentJS",
			"glob": {
				"pattern": "{docs,tags,lib,tasks}/**/*.{js,md}",
				"ignore": "lib/{configured,generate,find,generators/html}/test/**/*"
			}
		},
		"styles": {
			"parent": "Styles",
			"ignoreTemplateRender": "false",
			"glob": {
				"pattern": "{styles,site/default/static/styles}/**/*.{less,css,md}"
			}
		},
		"examples/multi": {
			"parent": "multi",
			"glob": "examples/multi/**/*.{js,md}"
		},
		"examples/simple": {
			"parent": "myproject",
			"glob": "examples/simple/**/*.{js,md}"
		}
	}
}